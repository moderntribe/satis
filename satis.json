{
  "name": "moderntribe/satis",
  "description": "Modern Tribe's Satis Server",
  "homepage": "https://moderntribe.github.io/satis/",
  "archive": {
    "directory": "dist",
    "format": "tar",
    "skip-dev": true
  },
  "repositories": [
    {
      "type": "package",
      "package": {
        "name": "advanced-custom-fields/advanced-custom-fields-pro",
        "version": "5.9.8",
        "type": "wordpress-plugin",
        "dist": {
          "type": "zip",
          "url": "https://connect.advancedcustomfields.com/index.php?a=download&p=pro&k={%WP_PLUGIN_ACF_KEY}&t={%VERSION}"
        },
        "require": {
          "ffraenz/private-composer-installer": "^5.0"
        }
      }
    }
  ],
  "require-all": true
}